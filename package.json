{
  "name": "imicros-flow",
  "version": "0.3.1",
  "author": "Andreas Leinen <andreas.leinen@imicros.de> (www.imicros.de)",
  "license": "MIT",
  "description": "Process engine of imicros framework",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage --no-cache",
    "test-local": "jest --coverage --no-cache --silent --config=local.js",
    "test-travis": "jest --coverage --no-cache --config=travis.js",
    "test-token": "jest test/flow.token.spec.js --no-cache --config=local.js",
    "test-event": "jest test/flow.event.spec.js --no-cache --config=local.js",
    "test-next": "jest test/flow.next.spec.js --no-cache --config=local.js",
    "test-t": "jest test/token.spec.js --no-cache --config=local.js",
    "test-activity": "jest test/activity.spec.js --no-cache --config=local.js",
    "test-sequence": "jest test/sequence.spec.js --no-cache --config=local.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/*.js",
      "lib/util/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dev/"
    ],
    "rootDir": ".",
    "roots": [
      "./test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/al66/imicros-flow.git"
  },
  "keywords": [
    "moleculer",
    "process",
    "engine"
  ],
  "devDependencies": {
    "benchmark": "^2.1.4",
    "imicros-acl": "^0.0.6",
    "jest": "^25.5.4",
    "moleculer": "^0.14.6",
    "nats": "^1.4.9"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "uuid": "^8.0.0",
    "imicros-flow-control": "^0.0.3",
    "imicros-streams": "^0.0.3"
  }
}
